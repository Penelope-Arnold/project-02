      <br>
  <section id="welcome-msg">
      <h1 class="lead mb-3">Welcome <%= name %>! </h1>
  </section>
  <br>
		<div class="row">
      <div class="col-4">
				<form id="post-form">
					<div class="form-group">
            <h2>Post a New Trip</h2>
						<label for="title">City:</label>
						<input id="city" placeholder="Post City" type="text" class="form-control" id="title">
                        <label id="country" for="title">Country:</label>
                        <input id="country" placeholder="Post Country" type="text" class="form-control" id="title">
                        <label for="title">Category:</label>
                        <input id="category" placeholder="Select Country" type="text" class="form-control" id="title">
                        <br />
                        <label for="body">Description:</label>
                        <textarea id="description" placeholder="Post Description" class="form-control" rows="10" id="body"></textarea>
                        <label for="title">Link to Photo:</label>
                        <input id="photo" placeholder="Upload Photo" type="text" class="form-control" id="title">
						<button id="submit-post" type="submit" class="btn btn-success submit">Submit</button>
					</div>
				</form>
      </div>
      <div class="col-1"></div>
      <div class="col-5 post-section overflow-auto postontainer">
        <div style="height: 290px;" class="row post-section overflow-auto">
        <h2>My Trips:</h2>
          <div class="card mb-3">
              <!-- <img src="..." class="card-img-top" alt="..."> -->
              <div class="card-body">
                <h5 class="card-title">Card title</h5>
                <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
                <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
              </div>
            </div>
            <div class="card">
